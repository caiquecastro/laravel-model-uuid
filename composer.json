{
    "name": "dyrynda/laravel-model-uuid",
    "description": "This package allows you to easily work with UUIDs in your Laravel models.",
    "license": "MIT",
    "keywords": [
        "eloquent",
        "laravel",
        "model",
        "uuid"
    ],
    "authors": [
        {
            "name": "Michael Dyrynda",
            "email": "michael@dyrynda.com.au",
            "homepage": "https://dyrynda.com.au"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "illuminate/database": "^7.3",
        "illuminate/events": "^7.3",
        "illuminate/support": "^7.3",
        "ramsey/uuid": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Dyrynda\\Database\\Support\\": "src/"
        }
    },
    "require-dev": {
        "dyrynda/laravel-efficient-uuid": "^4.0",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^8.0",
        "nunomaduro/collision": "^4.1"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "suggest": {
        "dyrynda/laravel-efficient-uuid": "Override Laravel's default query grammar to efficiently store UUIDs."
    },
    "minimum-stability": "stable"
}
